{
  "name": "twc/code-quality-plugin",
  "description": "pre-configure fixer and analyse code quality",
  "type": "composer-plugin",
  "license": "MIT",
  "require": {
    "php": "^7.2",
    "composer-plugin-api": "^2.0",
    "composer/composer": "^2.0",
    "symfony/framework-bundle": "^4.0|^5.0",
    "symfony/process": "^4.0|^5.0",
    "symfony/filesystem": "^4.0|^5.0",
    "symfony/finder": "^4.0|^5.0",
    "twc/phpinsights": "^1.15",
    "phpstan/phpstan": "^0.12.4"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.0"
  },
  "extra": {
    "class": "Twc\\Plugin\\Composer\\CodeQualityPlugin"
  },
  "autoload": {
    "psr-4": {
      "Twc\\Plugin\\Composer\\": "src",
      "Twc\\Plugin\\Composer\\Test\\": "tests"
    }
  }
}